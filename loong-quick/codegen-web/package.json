{
  "name": "codegen-web",
  "version": "1.0.0",
  "description": "codegen-server web前端",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "codegen": "codegen gen -config"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "art-template": "^4.13.2",
    "feign-boot-stater": "^1.0.5",
    "fengwuxp-spring-context": "1.0.5",
    "fengwuxp-spring-router": "^1.0.5",
    "react-helmet-async": "^1.0.7",
    "react-inlinesvg": "^2.2.2",
    "umi": "^3.3.9",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.3.9",
    "feign-boot-browser-stater": "^1.0.0",
    "fengwuxp-babel-dependency": "^1.0.0",
    "fengwuxp-browser-router": "^1.0.0",
    "fengwuxp-browser-storage": "^1.0.0",
    "fengwuxp-codegen-client": "^1.0.0",
    "fengwuxp-common-utils": "^1.0.0",
    "fengwuxp-declarative-router-adapter": "^1.0.0",
    "fengwuxp-declarative-storage-adapter": "^1.0.0",
    "fengwuxp-event-state": "^1.0.0",
    "fengwuxp-jest-dependency": "^1.0.0",
    "fengwuxp-react-dependency": "^1.0.0",
    "fengwuxp-routing-antd": "^1.0.0",
    "fengwuxp-routing-core": "^1.0.0",
    "fengwuxp-routing-react": "^1.0.0",
    "fengwuxp-spring-react": "^1.0.3",
    "fengwuxp-styles-dependency": "^1.0.0",
    "fengwuxp-typescipt-dependency": "^1.0.0",
    "fengwuxp-typescript-feign": "^1.0.0",
    "lint-staged": "^10.0.7",
    "log4js": "^6.3.0",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
